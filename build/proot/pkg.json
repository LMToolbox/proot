{
  "download": {
    "url": "https://github.com/proot-me/proot.git",
    "branch": "master"
  },
  "build": {
    "configure": [],
     "make": [
       "export LDFLAGS=\"$LDFLAGS -llog -lc -lm -lz -lpython3.13\" PYTHON=\"$PREFIX/bin/python3\" CFLAGS=\"$CFLAGS -I$PREFIX/include/python3.13\" &&",
        "make -C src loader.elf loader-m32.elf build.h &&",
        "make -C src proot care"
     ]
  },
  "export": [
    "src/proot",
    "src/care"
  ]
}
